{
  "name": "spacy",
  "namespace": false,
  "reason": {
    "react-jsx": 3
  },
  "bs-dependencies": [
    "@rescript/react",
    "@rescriptbr/ancestor",
    "@rescriptbr/radix-ui",
    "@rescriptbr/reform",
    "@rescriptbr/reschema",
    "rescript-js",
    "rescript-next"
  ],
  "ppx-flags": [
    "lenses-ppx/ppx"
  ],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "warnings": {
    "error": "+8"
  },
  "suffix": ".bs.js",
  "bsc-flags": [
    "-bs-g",
    "-bs-super-errors",
    "-open ReScriptJs.Js"
  ]
}
